
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = []
}

model Project {
  id          String   @id @default(cuid())
  name        String
  description String   @db.Text
  imageSrc    String
  category    String
  
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}